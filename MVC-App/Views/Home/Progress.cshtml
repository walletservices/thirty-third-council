@model DataTable
@using  System.Data;
@{
    ViewData["Title"] = "Progress";
}
<h1>@ViewData["Title"]</h1>



@ViewData["Progress"]
<table class="table table-hover">
    <thead>
        <tr>
            @foreach (DataColumn col in Model.Columns)
            {
                <th>@col.ColumnName</th>
            }
        </tr>
    </thead>
    <tbody>
        @foreach (DataRow row in Model.Rows)
        {
            <tr>
                @foreach (DataColumn col in Model.Columns)
                {
                    <td>@row[col.ColumnName]</td>
                }
            </tr>
        }
    </tbody>
    </table>
<p>Progress to be displayed like the executor.</p>


<div class="form-inline">
    <div>Step 1 - Done </div>
    <div>Step 2 - In Progress </div>
    <div>Step 3 - Todo </div>
    <div>Step 4 -To DO </div>

</div>
